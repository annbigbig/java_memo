package com.kashu.property.editors;

import java.beans.PropertyEditorSupport;

import com.howtodoinjava.demo.model.DepartmentEntity;
import com.kashu.domain.Category;

public class CategoryEditor extends PropertyEditorSupport {
    @Override
    public void setAsText(String id) 
    {
    	Category c;
    	switch(Integer.parseInt(id))
		{
			case 1: c = new Category(1,  "Jam"); break;
			case 2: c = new DepartmentEntity(2,  "Finance"); break;
			case 3: c = new DepartmentEntity(3,  "Information Technology"); break;
			default: c = null;
		}
        this.setValue(c);
    }
}
