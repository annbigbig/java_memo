package com.kashu.demo;

public class Rectangle extends Shape implements ShapeInfo,ShapeInterface {
	private double x1;
	private double y1;

	public Rectangle(double x,double y,double x1,double y1){
		super.x = x;
		super.y = y;
		this.x1 = x1;
		this.y1 = y1;
	}
	
	@Override
	public double area() {
		return (Math.abs(x-x1) * Math.abs(y-y1));
	}

	@Override
	public void show() {
		
		
	}

}
