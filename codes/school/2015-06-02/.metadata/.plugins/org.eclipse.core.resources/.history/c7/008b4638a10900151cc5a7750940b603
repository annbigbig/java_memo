package com.kashu.demo;

public class Multiple9to9 {

	public static void main(String[] args) {
		//variables declaration
		String str = "";
		
		//collect data
		
		
		//calculating
		for(int i=1;i<=9;i++){
			for(int j=1;j<=9;j++){
				if(j%3!=0){
					str = i + " * " + j + " = " + (i*j) + " ";
				  //System.out.print(i + " * " + j + " = " + (i*j) + " ");
				}else{
					System.out.println(i + " * " + j + " = " + (i*j) + " ");
		    	}
		    }
		     if(i%3==0){
		    	     System.out.println("");
		              }
		}
		
		
		//display results

	}

}
