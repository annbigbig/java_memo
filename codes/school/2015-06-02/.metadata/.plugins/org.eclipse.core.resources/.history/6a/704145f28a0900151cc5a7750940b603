package com.kashu.demo;

public class Multiple9to9 {

	public static void main(String[] args) {
		//variables declaration
		
		
		//collect data
		
		
		//calculating
		for(int i=1;i<=9;i+=3){
			for(int j=1;j<=9;j++){
				if(j%3!=0){
				  System.out.print(i + " * " + j + " = " + (i*j));
				}else
					System.out.print(i + " * " + j + " = " + (i*j));
			}
		}
		
		
		//display results

	}

}
