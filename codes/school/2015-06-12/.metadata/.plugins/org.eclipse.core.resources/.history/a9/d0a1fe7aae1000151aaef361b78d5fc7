package com.kashu.demo;

import java.util.Scanner;

public class Homework01 {

	public static void main(String[] args) {
		//variables declaration
		Scanner sc = new Scanner(System.in);
		int n[] = new int[10];
		int max = 0 , avg = 0;
		double total = 0;
		
		//collect data
		for(int i=0;i<10;i++){
			System.out.println("請輸入第" + (i+1) + "個整數");
			n[i] = sc.nextInt();
		}
		
		//calculating
		max = n[0];
		for(int j=1;j<10;j++){
			if(max<n[j]) max = n[j];
		}
		
		
		//display results

	}

}
