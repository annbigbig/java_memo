package com.kashu.demo;

public class Homework02 {

	public static void main(String[] args) {
		//variables declaration
		int A[][] = {{1,2,3},{4,5,6}};
		int B[][] = {{7,8,9},{10,11,12}};
		int C[][] = new int[2][3];
		
		//collect data
		
		
		//calculating
		for(int i=0;i<2;i++){
			for(int j=0;j<3;j++){
				C[i][j] = A[i][j] + B[i][j];
			}
		}
		
		//display results
		for(int k=0;k<2;k++){
			for(int m=0;m<3;m++){
				System.out.printf("%d",A[k][m]);
			}
		}

	}

}
