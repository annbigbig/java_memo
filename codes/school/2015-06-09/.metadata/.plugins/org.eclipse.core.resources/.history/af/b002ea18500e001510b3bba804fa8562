package com.kashu.demo;

import java.util.Scanner;

public class Fibonacci_ForLoop {

	public static void main(String[] args) {
		//variables declaration
		Scanner sc = new Scanner(System.in);
		int n = 0 , fn_2 = 0, fn_1 = 0, fn = 0;
		
		//collect data
		System.out.println("請問要求費氏數列第幾項呢？");
		n = sc.nextInt();
		
		//calculating
		if(n<=0) n=1;
		for(int i=1;i<=n;i++){
			if(i==1){
      fn = 1; fn_1 = 0; fn_2 = 0;
			}else if(i==2){
				fn = 1; fn_1 = 1; fn_2 = 0;
			}
		}
		
		//display results
		
	}

}
