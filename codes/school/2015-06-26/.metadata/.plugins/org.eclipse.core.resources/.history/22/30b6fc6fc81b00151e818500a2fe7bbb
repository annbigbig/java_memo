package com.kashu.demo;

public abstract class Food {
	private int amount;
	private int calorie;
	
	public Food(int gram){
		
	}
	
	public void setCaloriePerGram(int cal){
		this.calorie = cal;
	}
	
	public int getAmount(){
		return this.amount;
	}
	
	public int getCalorie(){
		return this.calorie;
	}
}
