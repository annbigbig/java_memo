package com.kashu.demo;

import java.io.BufferedReader;
import java.io.InputStreamReader;
import java.util.Scanner;

public class Sqrt {

	public static void main(String[] args) {
		   //variables declaration
		  int x=0,a=0,b=0,c=0;
		  Scanner sc = new Scanner(System.in);
		  boolean check_condition1 = false , check_condition2 = false;
		
		   //collect data
		  System.out.println("請輸入整數a");
		  a = sc.nextInt();
		  System.out.println("請輸入整數b");
		  b = sc.nextInt();
		  System.out.println("請輸入整數c");
		  c = sc.nextInt();
		  
		  //calculating
		  check_condition1 = (a>0);
		  check_condition2 = Math.sqrt((Math.pow(b, 2) - (double)(4 * a * c)))>0;
		     
		
		  //display results
		
	}

}
