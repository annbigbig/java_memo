package com.kashu.demo;

public class Ex2 {

	public static void main(String[] args) {
		double y = 3.21384;
		System.out.printf("f(y) = %.4f\n",f(y));
		System.out.printf("f(%.4f\n) = %.4f\n",f(-2.1));
	}

	public static double f(double x){
		double result = 0.0;
		result = x*((3*x*x) + 2) -1.0;
		return result;
	}
}
