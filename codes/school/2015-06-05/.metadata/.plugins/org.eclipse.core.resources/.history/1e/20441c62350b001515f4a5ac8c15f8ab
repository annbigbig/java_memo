package com.kashu.demo;

public class Ex6 {

	public static void main(String[] args) {
		//variables declaration
		
		
		//collect data
		
		
		//calculating
		for(int i=1;i<=9;i++){
			for(int j=1;j<=9;j++){
				System.out.printf("%2d * %2d = %2d",i,j,(i*j));
			}
		}
		
		//display results

		
	}

}
