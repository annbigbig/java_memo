package com.kashu.demo;

public class Ex6 {

	public static void main(String[] args) {
		//variables declaration
		
		
		//collect data
		
		
		//calculating
		for(int i=1;i<=9;i++){
			for(int j=1;j<=9;j++){
				if(j%3==0){
   			System.out.printf("%2d * %2d = %2d \t\n ",i,j,(i*j));
				}else{
					System.out.printf("%2d * %2d = %2d \t ", i,j,(i*j));
				}
			}
			if(i%3==0){
				System.out.println("");
			}
		}
		
		//display results

		
	}

}
